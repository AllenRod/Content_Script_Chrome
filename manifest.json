{
	"manifest_version":2,
	
	"name":"URL check",
	"description":"check mouse over URL",
	"version":"1.0",
	
	"browser_action":{
		"default_popup":"popup.html"
	},
	
	"content_scripts":[
	{
		"matches":["http://www.reddit.com/*"],
		"js":["core.js"],
		"css":["core.css"]
	}
	],
	
	"web_accessible_resources": [
		"images/*.png"
	]
}
